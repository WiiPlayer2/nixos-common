{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:1081"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "all": {
        "ClusterId": "davmail",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "calendar": {
        "ClusterId": "davmail",
        "Match": {
          "Path": "users/{username}/calendar/"
        }
      }
    },
    "Clusters": {
      "davmail": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:1080/"
          }
        }
      }
    }
  }
}
